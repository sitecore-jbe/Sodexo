@using Sitecore.XA.Foundation.MarkupDecorator.Extensions
@using Sitecore.XA.Foundation.RenderingVariants.Extensions
@using Sitecore.XA.Foundation.RenderingVariants.Fields
@using Sitecore.XA.Foundation.SitecoreExtensions.Extensions
@using Sitecore.XA.Foundation.Variants.Abstractions.Fields
@using Sitecore.Mvc.Helpers
@using Sitecore.Mvc


@model Sitecore.XA.Foundation.Variants.Abstractions.Models.VariantsRenderingModel

@if (Model.DataSourceItem != null || Html.Sxa().IsEdit)
{
    <div @Html.Sxa().Component(Model.Rendering.RenderingCssClass ?? "SodexoEggLeftCallToAction", Model.Attributes)>
        <div class="columns py-5" style="color: #2A295C;">
            <div class="container egg-green-1" id="utilisateurs-homepage">
                <div class="row">
                    <div class="col-12 d-md-flex align-items-start">
                        <section class="col-md-6">
                            <div class="egg left egg-green-1" style="background-image: url('https://www.sodexo.fr/fstrz/r/s/www.sodexo.fr/app/themes/sodexo-child/src/assets/images/egg-green-1.svg?frz-v=189')"></div>
                            <div class="filigrane left mt-3 d-none d-lg-block"> @Html.Sitecore().Field("Audience Name")</div>
                            <div class="my-3"> <h2><strong>@Html.Sitecore().Field("First Title")</strong><br>@Html.Sitecore().Field("Second Title")</h2> </div>
                            <div class="mt-3 mb-4">
                                <p>@Html.Sitecore().Field("Intro")</p>
                            </div>
                            <div class="d-buttons mt-2 mb-4 justify-content-start " style="">
                                <div class="button mr-xs-4 text-center">
                                    @Html.Sitecore().Field("Call To Action 1 Image")
                                    <p style="text-align: center;">
                                        <span style="color: #283897;"><strong>@Html.Sitecore().Field("Call To Action 1 Title")</strong></span>
                                        <br>
                                        <span style="color: #283897;">@Html.Sitecore().Field("Call To Action 1 SubTitle")</span>
                                    </p>
                                    @Html.Sitecore().Field("Call To Action 1 Link", Model.Item, new { @class = "btn btn-red d-block mx-auto mx-md-0 d-block d-md-inline-block" })
                                </div>
                                <div class="button  text-center">
                                    @Html.Sitecore().Field("Call To Action 2 Image")
                                    <p style="text-align: center;">
                                        <span style="color: #283897;"><strong>@Html.Sitecore().Field("Call To Action 2 Title")</strong></span>
                                        <br>
                                        <span style="color: #283897;">@Html.Sitecore().Field("Call To Action 2 SubTitle")</span>
                                    </p>
                                    @Html.Sitecore().Field("Call To Action 2 Link", Model.Item, new { @class = "btn btn-white d-block mx-auto mx-md-0 d-block d-md-inline-block" })
                                    <div>
                                        <a href="https://apps.apple.com/fr/app/passrestaurant-la-carte-ch%C3%A8que-restaurant-sodexo/id928011571">
                                            <img alt="Badge Apple Store" class="store-badge d-block d-lg-inline mt-3 mx-auto mx-lg-0 lazyloaded" src="data:image/png;base64,">
                                        </a>
                                        <a href="https://play.google.com/store/apps/details?id=com.guerdons.passresto&amp;hl=fr">
                                            <img alt="Badge Google Play" class="store-badge d-block d-lg-inline mt-2 mt-lg-3 mx-auto mx-lg-0 lazyloaded" src="data:image/png;base64,">
                                        </a>
                                    </div>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                        </section>
                        <section class="col-md-6">
                            <div class="my-5 d-none d-sm-flex align-items-center justify-content-center">
                                @Html.Sitecore().Field("Image", Model.Item, new { @class = "img-fluid mx-auto lazyloaded" })
                            </div>
                            <div class="d-buttons mt-2 mb-4 justify-content-end" style=""> </div>
                            <div class="clearfix"></div>
                        </section>
                    </div>
                </div>
            </div>
        </div>

        <div class="component-content">
            @if (Model.DataSourceItem == null)
            {
                @Model.MessageIsEmpty
            }
            else
            {
                foreach (BaseVariantField variantField in Model.VariantFields)
                {
                    @Html.RenderingVariants().RenderVariant(variantField, Model.Item, Model.RenderingWebEditingParams, Model)
                }
            }
        </div>
    </div>
}